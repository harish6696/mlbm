    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debug case_fno",
            "type": "debugpy",
            "request": "launch",
            "program": "case_fno.py",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debug MY_case_fno",
            "type": "debugpy",
            "request": "launch",
            "program": "case_my_fno.py",
            "console": "integratedTerminal",
            "args": [
                "data=config_velocity_re_200_period_1.yaml",
                "output.output_name=DEBUG",
                "gpu_id=0",
                "train.validation.validation_pseudo_epochs=2",
                "output=default_output.yaml",
            ],
            "justMyCode": true
        },
        {
            "name": "Debug custom_dataset",
            "type": "debugpy",
            "request": "launch",
            "program": "case_feature_extraction.py",
            "console": "integratedTerminal",
            "args": [
                "data=config_vel_den_re_200-400.yaml",
                "output.output_name=DEBUG",
                "gpu_id=0",
                "train.validation.validation_pseudo_epochs=2",
                "output=default_output.yaml",
            ],
            "justMyCode": true
        },

    ]
